{
      "name": "HA Bolt DIY Integration",
      "version": "1.4.6",
      "slug": "ha_bolt_diy_integration",
      "description": "Bolt DIY Integration for Home Assistant",
      "arch": ["armv7", "aarch64", "amd64"],
      "startup": "application",
      "boot": "auto",
      "ingress": true,
      "ingress_port": 5173,
      "panel_icon": "mdi:robot",
      "homeassistant": "2023.1.0",
      "ports": {
        "5173/tcp": 5173
      },
      "ports_description": {
        "5173/tcp": "Bolt DIY web interface"
      },
      "map": ["config:rw"],
      "options": {
        "bolt_url": {
          "name": "Bolt URL",
          "description": "URL of your Bolt DIY instance",
          "default": "http://localhost:5173",
          "required": true
        }
      },
      "schema": {
        "bolt_url": "str"
      },
      "image": "ghcr.io/home-assistant/{machine}-proxy"
    }
